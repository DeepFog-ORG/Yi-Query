{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/(.*\\.(js|css|jpg|jpeg|png|gif|svg|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html", "status": 200 }
  ]
}