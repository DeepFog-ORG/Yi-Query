{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)\.(js|css|jpg|jpeg|png|gif|svg|ico)$", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}